<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="userDao">
 	<insert id="UserJoin" parameterType="userDto">
 		insert into sys.user values(#{user_id},#{user_pwd},#{user_email},#{user_nickname},default,default,default,#{user_phone})
 	</insert>
 	<select id="UserInfo" parameterType="String" resultType="userDto">
 		select * from sys.user where user_id = #{user_id}
 	</select>
 </mapper>
 